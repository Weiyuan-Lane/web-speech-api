{
  "name": "web-speech-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "publish": "npm run github-pages:precleanup && npm run github-pages:prodbuild && npm run github-pages:publish && npm run github-pages:cleanup",
    "github-pages:prodbuild": "export SPA_EXP_BUILD=true && next build && cp ./github-pages/_config.yml dist/_config.yml && cp ./github-pages/.nojekyll dist/.nojekyll",
    "github-pages:publish": "subpath-as-branch -p dist -b gh-pages",
    "github-pages:cleanup": "rm -rf dist",
    "github-pages:precleanup": "rm -rf .next dist"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.2",
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "postcss": "^8",
    "subpath-as-branch": "^1.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
